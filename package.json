{
	"name": "aitinerary",
	"version": "1.0.0",
	"description": "AI-powered travel itinerary planning assistant",
	"type": "module",
	"private": true,
	"scripts": {
		"start": "vite dev",
		"deploy": "vite build && wrangler deploy",
		"test": "vitest",
		"types": "wrangler types env.d.ts --include-runtime false",
		"format": "prettier --write .",
		"check": "prettier . --check && biome lint && tsc",
		"build": "vite build",
		"migrate:remote": "wrangler d1 migrations apply aitinerary-db --remote",
		"migrate:local": "wrangler d1 migrations apply aitinerary-db --local"
	},
	"keywords": [
		"travel",
		"itinerary",
		"ai",
		"cloudflare",
		"agents"
	],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "^2.2.4",
		"@cloudflare/puppeteer": "^1.0.4",
		"@cloudflare/vite-plugin": "1.13.7",
		"@cloudflare/vitest-pool-workers": "^0.9.7",
		"@cloudflare/workers-types": "^4.20250927.0",
		"@tailwindcss/vite": "^4.1.13",
		"@types/node": "^24.6.0",
		"@types/react": "^19.1.14",
		"@types/react-dom": "^19.1.9",
		"@vitejs/plugin-react": "^5.0.4",
		"prettier": "^3.6.2",
		"tailwindcss": "^4.1.13",
		"typescript": "^5.9.2",
		"vite": "^7.1.7",
		"vitest": "3.2.4",
		"wrangler": "^4.40.2"
	},
	"dependencies": {
		"@ai-sdk/openai": "^2.0.42",
		"@ai-sdk/react": "^2.0.56",
		"@ai-sdk/ui-utils": "^1.2.11",
		"@libsql/client": "^0.15.15",
		"@openai/agents": "^0.1.7",
		"@phosphor-icons/react": "^2.1.10",
		"@radix-ui/react-avatar": "^1.1.10",
		"@radix-ui/react-dropdown-menu": "^2.1.16",
		"@radix-ui/react-slot": "^1.2.3",
		"@radix-ui/react-switch": "^1.2.6",
		"@types/leaflet": "^1.9.20",
		"agents": "https://pkg.pr.new/cloudflare/agents@531",
		"ai": "^5.0.56",
		"better-auth": "^1.3.24",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"drizzle-kit": "^0.31.5",
		"drizzle-orm": "^0.44.5",
		"leaflet": "^1.9.4",
		"lucide-react": "^0.544.0",
		"marked": "^16.3.0",
		"motion": "^12.23.22",
		"openai": "^5.23.2",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"react-leaflet": "^5.0.0",
		"react-markdown": "^10.1.0",
		"react-router-dom": "^7.9.3",
		"remark-gfm": "^4.0.1",
		"tailwind-merge": "^3.3.1",
		"zod": "^3.25.76"
	}
}
